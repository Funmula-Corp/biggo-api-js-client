{
  "name": "biggo-api-js-client",
  "version": "1.0.0",
  "description": "biggo api client for js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:esm; npm run build:cjs",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --outdir=./dist --allow-overwrite --format=esm --out-extension:.js=.mjs",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --outdir=./dist --allow-overwrite --format=cjs --out-extension:.js=.cjs"
  },
  "author": "Funmula Corp<info@funmula.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "esbuild": "^0.17.8",
    "typescript": "^4.9.5"
  }
}
