{
  "name": "biggo-api",
  "version": "0.1.0",
  "description": "biggo api client for node js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./api/*": {
      "import": "./lib/api/*/index.js",
      "require": "./lib/api/*/index.js",
      "types": "./lib/api/*/index.d.ts"
    },
    "./jwt": {
      "import": "./lib/auth/jwt/index.js",
      "require": "./lib/auth/jwt/index.js",
      "types": "./lib/auth/jwt/index.d.ts"
    }
  },
  "files": [
    "**/*.d.ts",
    "**/*.d.ts.map",
    "**/*.js",
    "**/*.js.map",
    "!node_modules"
  ],
  "scripts": {
    "ts": "dotenv -e .env.local -e .env -- ts-node --esm --experimental-specifier-resolution=node",
    "build": "dotenv -e .env -- tsc && dotenv -e .env -- node utils/replace-variable.cjs",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run build"
  },
  "author": "Funmula Corp.",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@types/node": "^18.13.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "replace-in-file": "^6.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
